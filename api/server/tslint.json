{
	"extends": "tslint-config-airbnb",
	"rules": {
		"align": false,
		"import-name": [false],
		"indent": [false, "tabs"],
		"max-line-length": [true, 160],
		"no-any": [true],
		"no-console": [true],
		"ter-indent": [false, "tab"],
		"trailing-comma": [
			true,
			{
				"singleline": "never",
				"multiline": {
					"arrays": "always",
					"exports": "always",
					"functions": "never",
					"imports": "always",
					"objects": "always",
					"typeLiterals": "ignore"
				}
			}
		],
		"variable-name": {
			"options": [
				"allow-pascal-case"
			]
		},
		"object-shorthand-properties-first": false,
		"prefer-array-literal": false
	}
}
