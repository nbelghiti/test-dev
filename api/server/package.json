{
  "name": "asset_database_node_app",
  "version": "1.10.0",
  "description": "Asset Database",
  "main": "index.ts",
  "scripts": {
    "start": "yarn serve",
    "serve": "ts-node-dev -r tsconfig-paths/register --transpileOnly --poll --inspect=0.0.0.0:5858 -- index.ts",
    "prebuild": "yarn clean:build",
    "build": "tsc -p tsconfig.json",
    "postbuild": "ts-node-dev scripts/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "lint:fix": "yarn lint --fix",
    "pretest": "yarn clean:coverage && yarn clean:db",
    "test": "jest --config jest.config.js --runInBand",
    "test:watch": "yarn test --watchAll --coverage false",
    "posttest": "yarn lint",
    "clean:build": "rm -rf ./dist",
    "clean:coverage": "rm -rf ./test/coverage",
    "clean:db": "ts-node -r tsconfig-paths/register ./test/setup",
    "open:coverage": "open ./test/coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@services/field-services": "3.2.0",
    "@studiohyperdrive/logger": "1.1.1",
    "@studiohyperdrive/swagger-express-ts": "0.0.17",
    "@types/bluebird": "3.5.28",
    "@types/multer": "1.3.10",
    "@types/semver": "^7.3.4",
    "@types/umzug": "2.2.2",
    "@types/validator": "10.11.3",
    "auth0": "2.24.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "csv-parser": "2.3.2",
    "express": "4.17.1",
    "express-jwt": "^5.3.1",
    "helmet": "3.21.1",
    "joi": "14.3.1",
    "json2csv": "^5.0.1",
    "jsondiffpatch": "0.4.1",
    "jwks-rsa": "^1.6.0",
    "multer": "1.4.2",
    "pg": "7.12.1",
    "pg-hstore": "2.3.3",
    "qs": "6.9.1",
    "ramda": "0.26.1",
    "reflect-metadata": "0.1.13",
    "semver": "7.3.2",
    "sequelize": "5.19.8",
    "sequelize-typescript": "1.0.0",
    "slugify": "1.3.6",
    "socket.io": "2.3.0",
    "swagger-ui-express": "4.1.2",
    "tsconfig-paths": "3.9.0",
    "umzug": "2.2.0",
    "uuid": "3.3.3",
    "xstate": "4.8.0"
  },
  "devDependencies": {
    "@types/auth0": "2.20.3",
    "@types/body-parser": "1.17.1",
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/helmet": "0.0.44",
    "@types/jest": "24.0.19",
    "@types/joi": "14.3.3",
    "@types/json2csv": "^5.0.0",
    "@types/node": "12.11.0",
    "@types/qs": "6.9.1",
    "@types/ramda": "0.26.32",
    "@types/sequelize": "4.28.6",
    "@types/shelljs": "0.8.6",
    "@types/sinon": "7.5.0",
    "@types/sinon-express-mock": "1.3.7",
    "@types/socket.io": "2.1.4",
    "@types/supertest": "2.0.8",
    "@types/swagger-ui-express": "4.1.2",
    "@types/uuid": "3.4.5",
    "dotenv": "8.2.0",
    "jest": "24.8.0",
    "jest-extended": "0.11.2",
    "shelljs": "0.8.3",
    "sinon": "7.5.0",
    "sinon-express-mock": "2.2.1",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-node-dev": "1.0.0-pre.43",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.8.3"
  },
  "license": "ISC"
}
